<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>gpum@mooz.github.com</em:id>
        <em:type>2</em:type>
        <em:version>0.2.0</em:version>
        <em:name>gpum - Google mail Popup Unread Manager</em:name>
        <em:creator>mooz</em:creator>
        <em:developer>mooz</em:developer>
        <em:homepageURL>http://github.com/mooz/gpum/</em:homepageURL>
        <em:optionsURL>chrome://gpum/content/config.xul</em:optionsURL>
        <em:iconURL>chrome://gpum/skin/icon32/gpum.png</em:iconURL>
        <em:unpack>true</em:unpack>
        <!-- en-US -->
        <em:description>Manage unread mail in the Google mail</em:description>
        <!-- ja -->
        <em:localized>
            <Description>
                <em:locale>ja</em:locale>
                <em:name>gpum - Google mail Popup Unread Manager</em:name>
                <em:creator>mooz</em:creator>
                <em:description>Google mail の未読メールをポップアップで管理</em:description>
                <em:homepageURL>http://github.com/mooz/gpum/</em:homepageURL>
            </Description>
        </em:localized>
        <!-- Firefox -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>15.0</em:minVersion>
                <em:maxVersion>15.*</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>

</RDF>
